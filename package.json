{
  "name": "my-vite-app",
  "private": true,
  "devDependencies": {
    "@stylistic/eslint-plugin": "^4.4.0",
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.18",
    "lerna": "^8.2.2",
    "prisma": "^6.9.0",
    "ts-node-dev": "^2.0.0"
  },
  "scripts": {
    "lint": "lerna run lint --stream --since",
    "lint:fix": "lerna run lint:fix --stream --since",
